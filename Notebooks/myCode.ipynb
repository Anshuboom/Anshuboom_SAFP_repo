    "def proximity_in_KM(myloc, attackloc):\n",
    "    from geopy.distance import distance\n",
    "\n",
    "    # Define the coordinates of the two points\n",
    "    point1 = myloc \n",
    "    point2 = attackloc  \n",
    "    \n",
    "    # Compute the distance between the two points\n",
    "    dist = distance(point1, point2).km\n",
    "    return dist\n",
