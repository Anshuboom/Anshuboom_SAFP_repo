    "        def convert_to_list(input):\n",
    "            if isinstance(input, list):\n",
    "                return input\n",
    "            elif isinstance(input, str):\n",
    "                return ast.literal_eval(input)\n",
    "            else:\n",
    "                raise ValueError(\"Input must be a list or a string representation of a list.\")\n",
    "        \n",
